                          <li><a href="" class="a_sx" id="<%=data.belongstoNum[5]._id%>">Research Institute of IntelligenceTechnology and System Integra (<%=data.belongstoNum[5].num%>)</a></li>
                         <!-- <li><a href="" class="a_sx" id="<%=data.belongstoNum[8]._id%>">Departments(<%=data.belongstoNum[8].num%>)</a></li> -->
