                                        </div><!-- row 2  -->
                                    </div><!-- page 1 page 2 -->
                            <%})%>
                        </div><!-- page-list -->
                    <%}%>
                    <div class="page-num">
                        <% for(var i=0;i < totalpage ;i++){%>
                            <span class="num"><%=i+1%></span>
                         <%}%>
                    </div>  
                </div>
                <script type="text/javascript"> 
                    function getQueryString(name) {
                        var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
                        var r = window.location.search.substr(1).match(reg);
                        if (r != null) {
                        return unescape(r[2]);
                        }
                        return null;
                    }
                    $().ready(function() {  
                        //三级菜单
                        let check_zc = getQueryString('zc')
                        console.log('check_zc-----',check_zc,typeof(check_zc))
                        if(check_zc=='6'||check_zc=='9'||check_zc=='10'){
                            $('.third').addClass('active')
                        }
                        $('.third').click(function(){
                            $(this).addClass('active')
                        }) 
                        $('#search').bind('keydown',function(event){
                            if(event.keyCode == "13") {
                                console.log('回车搜索',$('#search').val())
                                let reg = new RegExp("index", "g"); 
                                let url = "index"; 
                                window.location.href = url.replace(reg, 'index?p=1&s='+$('#search').val()); 
                            }
                        }); 
                        $('.search-img').click(function(){
                            console.log('search')
                            if(!$('#search').val())
                            return
                            let reg = new RegExp("index", "g"); 
                            let url = "index"; 
                            window.location.href = url.replace(reg, 'index?p=1&s='+$('#search').val());  
                        })
                        if('<%=search_txt%>'){
                         $('#search').val('<%=search_txt%>')
                        }  
                        $('.choose_i').click(function(){
                            console.log('dddddddddddddd')
                            let reg = new RegExp("index", "g"); 
                            let url = "index"; 
                            window.location.href = url.replace(reg, 'index?p=1&i='+$(this).text());
                        })
                        $('.choose_i').each(function(index,obj){
                            if('<%=zimu%>' == $(obj).text()){
                                console.log('找到page,点中状态',$(obj).text())
                            $(obj).css({'color':'#cca84d'})
                          }
                        })
                        $('#clear_choose').click(function(){
                            let reg = new RegExp("index", "g"); 
                            let url = "index"; 
                            window.location.href = url.replace(reg, "index"); 
                        })    
                        $('.num').each(function(index,obj){
                          if('<%=page%>' == $(obj).text()){
                            console.log('找到page,点中状态',$(obj).text())
                            $(obj).addClass('active')
                          }
                        })
                        $('.num').click(function(){
                            console.log('点击页面---->',$(this).text(),'构造url')        
                            let reg = new RegExp("index", "g"); 
                            let url = "index"; 
                            console.log(url.replace(reg, "index?p=" + $(this).text()))
                            window.location.href = url.replace(reg, "index?p=" + $(this).text() +'&zc=<%=zhicheng%>&sx=<%=suoxi%>&s=<%=search_txt%>&i=<%=zimu%>'); 
                        })
                        $('.a_zc').click(function(){
                            console.log('点击zhicheng---->',$(this),'点中状态，前端更新结果') 
                            let reg = new RegExp("index", "g"); 
                            let url = "index";                                 
                            window.location.href = url.replace(reg, 'index?p=1&zc='+$(this).attr('id')); 
                            $(this).parent().siblings().removeClass('active')
                            $(this).parent().addClass('active')
                            return false
                        })
                        $('.a_zc').each(function(index,obj){
                            console.log('传回来的职称----><%=zhicheng%>')
                            
                            if('<%=zhicheng%>' == $(obj).attr('id')){
                                console.log('找到,点中状态',$(obj).text())
                                if($(obj).attr('id')==6||$(obj).attr('id')==9||$(obj).attr('id')==10){
                                    $(obj).addClass('active')
                                }
                                $(obj).parent().addClass('active')
                            }
                        })
                        $('.a_sx').click(function(){
                            console.log('点击suoxi---->',$(this),'点中状态，前端更新结果') 
                            let reg = new RegExp("index", "g"); 
                            let url = "index";                                 
                            window.location.href = url.replace(reg, 'index?p=1&sx='+$(this).attr('id')); 
                            $(this).parent().siblings().removeClass('active')
                            $(this).parent().addClass('active')
                            return false
                        })
                        $('.a_sx').each(function(index,obj){
                            console.log($(obj).attr('id'))
                            if('<%=suoxi%>' == $(obj).attr('id')){
                                console.log('找到,点中状态',$(obj).text())
                                $(obj).parent().addClass('active')
                            }
                        })
                    })
                </script>
            </div>
        </div>
    </div>
  </div>
  
