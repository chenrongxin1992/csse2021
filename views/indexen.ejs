<%if(L=='1'){%>
    <%include head.ejs%>
  <%}%>
  <%if(L!='1'){%>
    <%include headen.ejs%>
  <%}%>
<div class="banner">
    <div class="slider">
        <%data.slider.forEach(function(item,index){%>
        <a href="#">
          <div class="slider-img">
            <img class="lazy_img" src="<%=item.pic%>" />
          </div>
          <div class="banner-txt"><%=item.title1%></div>
        </a>
        <%})%>
    </div>
</div>
<div class="m-banner-txt"><%=data.slider[0].title1%></div>
<!-- pc适配 -->
<div class="contain">
    <div class="about">
        <img src="image/university_image/file.png" class="img-file">
        <div class="area-img">
            <div class="bg">
                <img src="image/index_en/index-en_03.png" class="img1" alt="">
                <img src="image/index_en/img1.png" class="img2" />
                <img src="image/index_en/huanghui.png" class="img3" />
                <img src="image/index_en/index-en_05.png" class="img4" alt="">
            </div>
            <p class="name">Dean Hui Huang</p>
        </div>
        <div class="area-desc">
            <div class="desc">
                <img src="image/index_en/left.png" class="img-left" />
                <div class="txt txt1">
                  <%-data.yzjy.pageContentEN-%>
                </div>
                <img src="image/index_en/right.png" class="img-right" />
            </div>
        </div>
    </div>
    <div class="bg-wrap">
        <div class="wrap"></div>
        <!-- 科研成果 -->
        <div class="research">
            <div class="img_bg">
                <img class="img pc lazy_img" src="image/index_en/research_bg.png" />
                <img class="img mb lazy_img" src="image/index/research_bg_m.png" />
                <img src="image/index/recruitment/ellipse_left.png" class="semicircle1 lazy_img" />
                <img src="image/index/recruitment/ellipse_right.png" class="semicircle2 lazy_img" />
                <img src="image/index/recruitment/ellipse_right.png" class="semicircle3 lazy_img" />

                <div class="research_wrap"></div>
                <div class="research_txt">
                    <p>CSRankings 2016-2022</p>
                    <p>Top-ranked in China</p>
                </div>
                <a class="research_btn" href="http://csrankings.org/" target="_blank">
                    CSRankings<img src="image/index/arrow-right.png" />
                </a>
            </div>
            <div class="research_cont">
                <div class="research_banner">
                    <div class="research_tit c_cca84d b">Research</div>
                    <div class="pc-slider slider">
                      <%data.kycg.forEach(function(item,index){%>
                        <%if(item.wzlink){%>
                            <a href="<%=item.wzlink%>" class="item" target="_blank"><img class="slider-img" src="<%=item.fujianPath%>" /></a>
                          <%}else{%>
                            <a href="/csse/pages/research/details?id=<%=item.id%>" class="item"><img class="slider-img" src="<%=item.fujianPath%>" /></a>
                          <%}%>
                        
                      <%})%>
                    </div>
                </div>
                <div class="research_desc">
                    <a class="c_cca84d" href="/csse/pages/research/details?id=<%=data.kycg[0].id%>"><%=data.kycg[0].title%></a>
                    <div class="c_666 txt">
                        <span class="kanwu"><%=data.kycg[0].kanwu%> <%=data.kycg[0].year%></span>
                        <span class="ky_zuozhe"><%=data.kycg[0].zuozhe%></span>
                        <div class="c_999"><%=data.kycg[0].belongsto1%></div>
                    </div>
                    <a class="more" href="/csse/pages/research/index">
                        More <img class="more_img" src="image/index/right.png" />
                    </a>
                </div>
            </div>
        </div>
        <!-- 招生招聘 -->
        <div class="recruitment">
            <div class="recruitment_img">
                <img src="image/index_en/index-en_04.png" class="lazy_img" />
            </div>
            <div class="recruitment_cont">
                <div class="tit">Admissions</div>
                <div class="r-list clearfix">
                    <div>
                        <a class="r-row active" href="https://lxs.szu.edu.cn/info/1168/2445.htm" target="_blank">
                            <img src="image/index_en/img11.png" class="img" />
                            <span class="txt">Undergraduates</span>
                        </a>
                    </div>
                    <div>
                        <a class="r-row" href="https://lxs.szu.edu.cn/info/1168/2444.htm" target="_blank">
                            <img src="image/index_en/img2.png" class="img" />
                            <span class="txt">M.Sc. Candidates</span>
                        </a>
                        <a class="r-row" href="https://lxs.szu.edu.cn/info/1168/2443.htm" target="_blank">
                            <img src="image/index_en/img3.png" class="img" />
                            <span class="txt">Ph.D. Candidates</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <!-- 国际合作 -->
        <div class="internationality">
            <div class="internationality_cont">
                <div class="tit c_cca84d b">Collaboration</div>
                <div class="internationality_logo">
                  <%data.hzhb.forEach(function(item,index){%>
                    <div class="img">
                        <img src="<%=item.fujianPath%>" />
                    </div>
                  <%})%>
                    
                </div>
                <a class="more" href="/csse/pages/globalCooperation/partner">
                    More <img class="more_img" src="image/index/right.png" />
                </a>
            </div>
            <div class="internationality_img">
                <img src="image/index/internationality_bg.png" class="img" />
                <img src="image/index/recruitment/ellipse_left.png" class="semicircle1" />
                <img src="image/index/recruitment/ellipse_right.png" class="semicircle2" />
                <img src="image/index/recruitment/ellipse_right.png" class="semicircle3" />
                <div class="internationality_wrap">
                    <img class="wrap-img" src="image/index/internationality/sky.png">
                </div>
            </div>
        </div>
    </div>
    <!-- high-lights -->
    <div class="high-lights">
        <!-- <span class="tit">Highlights</span> -->
        <div class="high-list">
            <div class="high-left clearfix">
                <div class="l l-30">
