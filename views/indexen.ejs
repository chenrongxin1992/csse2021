                <div class="l l-25">
                    <div class="img">
                        <img src="image/index/internationality/24@2x.png" />
                    </div>
                    <div class="num c_cca84d b mynum"><%=data.highlights.benks%></div>
                    <div class="small c_333399">Undergraduates</div>
                </div>
                <div class="l l-25">
                    <div class="img">
                        <img src="image/index/internationality/20@2x.png" />
                    </div>
                    <div class="num c_cca84d b mynum"><%=data.highlights.yanjs%></div>
