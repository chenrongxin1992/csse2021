                var thumb = $(slider.$slides[i]).data();
                return '<button></button>';
            },
        });
        // 科研成果切换事件
        $('.research_banner .slider').slick({
            arrows: true,
            autoplay: true,
            slidesToShow: 1,
            autoplaySpeed: 5000,
            pauseOnHover: true,
            lazyLoad: 'ondemand'
        });
        $('.research_banner .slider').on('afterChange', function (event, slick, currentSlide) {
            // currentSlide 为当前显示的slider下标
            console.log(currentSlide);
            $('.research_desc .c_cca84d').html(
                'A distributed data management system to support large-scale data analysis -- ' +
                currentSlide)
            $('.research_desc .c_666 span').html('The Journal of Systems & Software -- ' +
                currentSlide)
            $('.research_desc .c_999').html('大数据技术与应用研究所 -- ' + currentSlide)
        });
    });
