          } 
        },
          direction: 'horizontal', // 垂直切换选项
          loop: true, // 循环模式选项
          // effect:'flip',//flip
          autoplay : true,
          speed:1500,
          autoplayDisableOnInteraction :true,
          // 如果需要分页器
          pagination: {
            el: '.swiper-pagination',
          },
          
          // 如果需要前进后退按钮
          navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
          },
          
          // 如果需要滚动条
          scrollbar: {
            el: '.swiper-scrollbar',
          },
  })  
    //change language
    $('#changeL').click(function(){
      console.log('change to en')
      $.get('/csse/setLanguage', {
        language:'0'
      }, function(data){
        if(data.success){
          //return 1
          return window.location.reload();
        }
      }, 'json');
    })
    	$('#clock').jclock({ withDate: true, withWeek: true, fontSize: '12px' })
		$('.lazy_img, .lazy_bg').lazyload({
