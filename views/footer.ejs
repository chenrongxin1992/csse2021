            respondTo:'window',
            arrows:true,
            //autoplay:true,
            slidesToShow: 1,
            //autoplaySpeed: 10000,
            pauseOnHover:true,
            lazyLoad: 'ondemand',
            adaptiveHeight:true
        });
        $('.m-news .slider').slick({
            dots:true,
            arrows:false,
            autoplay:true,
            slidesToShow: 1,
